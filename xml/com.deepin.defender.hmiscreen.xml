<interface name="com.deepin.defender.hmiscreen">
    <method name="Show">
    </method>
    <method name="ShowModule">
    <arg name="module" type="s" direction="in"/>
    </method>
    <method name="ShowPage">
    <arg name="module" type="s" direction="in"/>
    <arg name="page" type="s" direction="in"/>
    </method>
    <method name="ExitApp">
    </method>
    <!-- 显示设置弹框 -->
    <method name="showSettingDialog">
      <arg name="sGroupKey" type="s" direction="in"/>
    </method>
    <!-- 右键扫描 -->
    <method name="showScanByPath">
    <arg name="sPath" type="s" direction="in"/>
    </method>

    <!-- 显示联网管控界面弹框 -->
    <method name="showFuncConnectNetControl">
    <arg name="sDkgName" type="s" direction="in"/>
    </method>
    <!-- 显示远程访问界面弹框 -->
    <method name="showFuncConnectRemControl">
    <arg name="sDkgName" type="s" direction="in"/>
    </method>
    <!-- 显示流量详情界面弹框 -->
    <method name="showFuncConnectDataUsage">
    </method>

</interface>
